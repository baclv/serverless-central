{
    "name": "sample-default",
    "version": "0.0.1",
    "main": "build/index.js",
    "types": "build/index.d.js",
    "license": "MIT",
    "scripts": {
        "build": "yarn run lint && tsc -p ./tsconfig.json",
        "lint": "../../node_modules/tslint/bin/tslint --fix src/**/*.ts",
        "clean": "rimraf ./build",
        "dev": "tsc -p ./tsconfig.json -w",
        "prebuild": "yarn run clean",
        "predev": "yarn run clean",
        "test": "jest -c ../../jest.json",
        "sls-deploy":
            "yarn run build && node --require ../../dotenv.js  ../../node_modules/serverless/bin/serverless deploy -v",
        "sls-remove":
            "node --require ../../dotenv.js  ../../node_modules/serverless/bin/serverless remove -v",
        "sls-remove-alias":
            "node --require ../../dotenv.js  ../../node_modules/serverless/bin/serverless alias remove -v",
        "sls-logs":
            "node --require ../../dotenv.js  ../../node_modules/serverless/bin/serverless logs -v -t"
    },
    "devDependencies": {
        "faker": "^4.1.0",
        "jest": "^21.2.1",
        "prettier": "^1.12.1",
        "rimraf": "^2.6.2",
        "serverless-domain-manager": "^2.3.6",
        "serverless-localstack": "^0.2.3",
        "serverless-offline": "^3.20.3",
        "serverless-offline-localstack": "^0.0.3",
        "ts-jest": "^21.1.3",
        "typescript": "^2.5.3"
    }
}
